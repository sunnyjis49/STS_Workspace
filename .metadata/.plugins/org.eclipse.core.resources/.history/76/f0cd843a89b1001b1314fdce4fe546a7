package com.sun.springcore.ref;

public class Addition {
	private int x;
	private int y;

	public Addition(int x, int y) {
		this.x = x;
		this.y = y;
		System.out.println("int : int constructor");
	}

	public Addition(double x, double y) {
		this.x = (int) x;
		this.y = (int) y;
		System.out.println("double : double constructor");
	}
	
	
}
